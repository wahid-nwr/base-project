<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
 <hibernate-mapping>
  <class name="com.swiftcorp.portal.beneficiary.dto.BeneficiaryDTO" table="beneficiary">
	  <id name="componentId" column="componentId" type="long">
	    <generator class="increment" /> 
	  </id>
	 
	 
	 <property name="beneficiaryId" type="string" column="beneficiaryId"/>
	 <property name="beneficiaryName" type="string" column="beneficiaryName"/>
	 	
	 <list name="beneficiaryExamDTOList" cascade="all" lazy="false">
				<key column="beneficiary"/>
				<index column="benExamOrder" />
				<one-to-many class="com.swiftcorp.portal.beneficiary.dto.BeneficiaryExamDTO" />
	 </list>
		   
	 <list name="beneficiaryHistoryDTOList" cascade="all" lazy="false">
				<key column="beneficiary"/>
				<index column="benficiaryHistOrder" />
				<one-to-many class="com.swiftcorp.portal.beneficiary.dto.BeneficiaryHistoryDTO" />
	 </list>
	 
	 
  </class>
   
 </hibernate-mapping>
 
 