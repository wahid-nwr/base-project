<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
 <hibernate-mapping>
 
  <class name="com.swiftcorp.portal.question.dto.AnswerTypeDTO" table="answertype">
	  <id name="componentId" column="componentId" type="long">
	    <generator class="increment" /> 
	  </id>
	 <property name="answerTypeName" type="string" column="answerTypeName" /> 
	 <property name="answerTypeId" type="int" column="answerTypeId"  /> 
      </class>
   
 </hibernate-mapping>
