<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.swiftcorp.portal.address.dto.AddressDTO" table="address">
		<id name="componentId" column="componentId" type="long" unsaved-value="1">
			<generator class="increment" /> 
		</id>
  		
		<property name="addressLine" type="string" column="addressLine"/>
		<property name="city" type="string" column="city"  /> 
		<property name="stateOrProvince" type="string" column="stateOrProvince"  /> 
		<property name="zipOrPostalCode" type="string" column="zipOrPostalCode"  /> 
		<property name="country" type="string" column="country"  /> 	
		 <property name="updatedDate" type="calendar" column="updateddate"  />	
		<property name="updatedBy" type="long" column="updatedby"  />
        <property name="status" type="int" column="status" /> 
		
	</class>
 
 
</hibernate-mapping>